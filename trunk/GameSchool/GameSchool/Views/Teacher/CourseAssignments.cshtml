@model GameSchool.Models.ViewModels.TeacherViewModels.CourseAssignmentsViewModel

@foreach (var level in Model.Levels)
{
    <div class="TeacherCourseAssignmentsTestsLectures">
        <h3>@level.NumberInCourse. @level.Name</h3>
        @foreach (var assignment in Model.Assignments)
	    {
            
                if (assignment.LevelID == level.ID)
                {
                    <div class="TeacherAssignmentsTestsLectures">
                        <p>@assignment.Name</p>
                    </div>
                }
            
	    }
    </div>
}