@model GameSchool.Models.ViewModels.NewsFeedViewModel
           
<p>Nýjustu fyrirlestrar</p>

<div class="tabbable">
<ul>
@foreach (var lectures in Model.Lectures)
{
    <!--<li>@lectures.DateAdded: @Html.ActionLink(lectures.Name, "GetLecture/" + lectures.ID, "Student")</li>-->
    <li>@lectures.DateAdded: <a href="#tab2" data-toggle="tab">@lectures.Name</a></li>
}
</ul>
</div>

<p>Nýjustu verkefni</p>
<ul>
@foreach (var assignments in Model.Assignments)
{
    <li>@Html.ActionLink(assignments.Name, "GetAssignment/" + assignments.ID, "Student")</li>
}
</ul>
